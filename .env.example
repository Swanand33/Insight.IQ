# InsightGenie Environment Variables

# Required: OpenAI API Key for AI analysis
OPENAI_API_KEY=sk-your-openai-api-key-here

# Optional: Application Settings
OPENAI_MODEL=gpt-4
MAX_FILE_SIZE_MB=50
CACHE_SIZE=100

# Optional: Performance Settings
MAX_VISUALIZATION_ROWS=10000
ENABLE_PERFORMANCE_MODE=true
LOG_LEVEL=INFO

# Optional: UI Settings
APP_TITLE="InsightGenie - AI Excel Analytics"
APP_DESCRIPTION="Transform spreadsheets into insights"

# Development Settings
DEBUG=false
DEV_MODE=false